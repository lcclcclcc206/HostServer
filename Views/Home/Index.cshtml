@using HostServer.Models;
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>

<h2 class="mb-3">文件目录</h2>
<ul>
    @foreach (var provider in Configuration.StaticFileProviderList)
    {

        <li>
            <a href="@provider.RequestPath">@provider.DisplayName</a>
        </li>
    }
</ul>